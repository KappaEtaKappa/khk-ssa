<div id="header">
	<div id="header-app">
		<span>
			<a href="/">
				The Delta
			</a>
		</span>
		<% if(locals.title) { %>
			<a href="#">
				<i class="fa fa-angle-right"></i>
				<span><%- title %></span>
			</a>
		<% } %>
	</div>
	<img src="">
	<% if(logged){ %>
		<div id="header-user-out">
			<a href="/signout"><span>Sign Out&nbsp;</span><i class="fa fa-sign-out"></i></a>
		</div>
		<div id="header-user">
				<% if(user.privilegeLevel == 2) { %>
					<i class="fa fa-bolt admin"></i>
				<% } %>
				<span><a href="#"><%- user.name %></a></span>
		</div>
	<% } else { %>
	<% } %>
</div>
