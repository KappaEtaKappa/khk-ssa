<% include inc/head %>
	<div id="applications">
		<% for(var i=0; i<apps.length; i++){ %>
			<div class="application">
				<% if(apps[i].image) { %>
					<img src="/img/<%- apps[i].image %>">
				<% } else { %>
					<i class="fa <%- apps[i].icon %>"></i>
				<% } %>
				<%- apps[i].name %>
			</div>
		<% } %>
	</div>
<% include inc/foot %>
